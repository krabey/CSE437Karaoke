{
  "manifest_version": 2,

  "name": "Karaoke Extension",
  "description": "Displays lyrics for YouTube songs",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Karaoke Extension"
  },
  "permissions": [
    "activeTab",
    "storage"
  ]
}